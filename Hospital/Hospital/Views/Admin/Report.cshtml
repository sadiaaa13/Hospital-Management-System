@model IEnumerable<Hospital.Models.Report>

<div class="container mt-5">
    <h1 class="text-center mb-4">Uploaded Reports</h1>
    <table class="table table-hover shadow">
        <thead class="thead-dark">
            <tr>
                <th>File Name</th>
                <th>Upload Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var report in Model)
            {
                <tr>
                    <td>@report.FileName</td>
                    <td>@report.DateTime_UploadDate</td>
                </tr>
            }
        </tbody>
    </table>
</div>
